{
  "name": "staffbase_e2e_test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean:reports": "rmdir /S /Q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mochareports",
    "pretest": "npm run clean:reports",
    "scripts": "cypress run",
    "combine-reports": "mochawesome-merge  cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test": "npm run scripts || npm run posttest",
    "triggerAllTests-headless": "npx cypress run",
    "triggerAllTests-chrome": "npx cypress run --browser chrome",
    "triggerAllTests-dashboard": "npx cypress run --record --key 07b38c00-ae70-494d-80a1-b7ebc7203f77",
    "triggerAllTests-staffbase": "npx cypress run --spec cypress/e2e/*",
    "triggerAllTests-staffbase-dashboard": "npx cypress run --spec cypress/e2e/* --record --key 07b38c00-ae70-494d-80a1-b7ebc7203f77 --parallel",
    "triggerAllTests-staging": "npx cypress run --env configFile=staging"
  },
  "keywords": [],
  "author": "Sarah Yoon",
  "license": "ISC",
  "devDependencies": {
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "cypress": "^10.7.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-mochawesome-reporter": "^3.2.2",
    "cypress-multi-reporters": "^1.6.1",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^12.2.0",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "rmdirp": "^0.1.0"
  }
}
